<odoo>
    <data>

        <record id="course_form_view" model="ir.ui.view">
            <field name="name">course_form_view</field>
            <field name="model">course.management</field>
            <field name="arch" type="xml">
                <form>
                    <sheet >
                        <div class="oe_title">
                            <label for="name"/>
                            <h3>
                                <field name="name" />
                            </h3>
                        </div>

                            <group string="info">
                                <field name="instructor_id"/>
                                <field name="room_id"/>
                                <field name="capacity"/>
                                <field name="description"/>
                            </group>
                              <group >
                                <field name="attendee_ids"/>
                                <field name="lesson_ids" create="0"/>
                            </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="course_tree_view" model="ir.ui.view">
            <field name="name">course_tree_view</field>
            <field name="model">course.management</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                         <field name="instructor_id"/>
                         <field name="capacity"/>
                          <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="course_action" model="ir.actions.act_window">
            <field name="name">course</field>
            <field name="res_model">course.management</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="course " id="course" parent="base_menu_courses" action="course_action" />

    </data>
</odoo>